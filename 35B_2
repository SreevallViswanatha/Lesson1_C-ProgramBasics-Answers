#include <stdio.h>
#include <stdlib.h>
#include <math.h>
//Print all Trigonometric ratios of an angle
int main()
{
    float angle, s, c, t;

    printf("\nEnter an angle: ");
    scanf("%f", &angle);

    //convert angle to radians
    angle = angle*3.14/180;

    s = sin(angle);
    c = cos(angle);
    t = tan(angle);

    printf("sin = %f cos = %f tan = %f\n", s, c, t);
}
